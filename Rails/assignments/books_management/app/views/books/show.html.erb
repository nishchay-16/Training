<h2>Books</h2>
<table class="table-container">
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Genre</th>  
      <th>Quantity</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.author %></td>
        <td><%= book.genre %></td>
        <td style="text-align: center;"><%= book.quantity %></td>
      </tr>
    <% end %>
  </tbody>
</table>